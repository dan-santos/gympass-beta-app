{
  "name": "node3",
  "version": "1.0.0",
  "description": "Gympass Beta App",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "test:create": "npm link ./prisma/vitest-environment-prisma",
    "test:install": "npm link vitest-environment-prisma",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "pretest:e2e": "run-s test:create test:install",
    "test:e2e": "vitest run --dir src/http",
    "dev": "tsx watch src/server.ts",
    "prd": "node build/server.js",
    "build": "tsup --out-dir build"
  },
  "keywords": [],
