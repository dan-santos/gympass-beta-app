    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "@vitest/coverage-v8": "0.34.6",
    "@vitest/ui": "0.34.6",
    "eslint": "8.50.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.4.1",
    "supertest": "6.3.3",
    "tsup": "7.2.0",
    "tsx": "3.13.0",
    "typescript": "5.2.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.33.0"
  },
  "dependencies": {
